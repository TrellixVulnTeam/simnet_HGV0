{
    "max_length": 500,
    "char_limit": 10,
    "scope":"HardAttention",
    "num_classes":3,

    "with_char": false,
    "char_emb_dim": 20,
    "char_lstm_dim": 40,

    "batch_size": 50,
    "max_epochs": 500,
    "dropout_rate": 0.4,
    "learning_rate": 1e-4,
    "optimizer": "adam",
    "lambda_l2": 0.0,
    "grad_clipper": 10.0,

    "context_layer_num": 1,
    "context_lstm_dim": 100,
    "hidden_units":100,

    "slstm_layer_num": 5,

    "rnn":"lstm",
    "random_initialize":false,
    "step":5,
    "slstm_hidden_size":300,

    "highway_layer_num": 1,
    "with_highway": true,
    "with_match_highway": true,
    "with_aggregation_highway": true,

    "use_cudnn": true,

    "with_moving_average": 0.999,

    "loss":"focal_loss_multi_v1",

    "alpha":0.5,
    "gamma":2.0,
    "name":"focal_loss_multi_v1",

    "scale":30,
    "margin":0.35,

    "weight_decay":1e-7,

    "num_heads":8,
    "with_self_attention":false,
    "with_multi_head_interaction":false,

    "apply_elmo":false,
    "elmo_pca":false,

    "num_filters":128,
    "num_heads":8,
    "hard_attention":"dot",
    "weight_constraint":"none"
}
